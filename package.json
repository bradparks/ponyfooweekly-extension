{
  "name": "ponyfooweekly-extension",
  "version": "1.0.1",
  "description": "Browser extension to easily submit link suggestions to Pony Foo Weekly",
  "repository": {
    "type": "git",
    "url": "https://github.com/ponyfoo/ponyfooweekly-extension.git"
  },
  "author": "Nicol√°s Bevacqua <nico@ponyfoo.com> (https://ponyfoo.com)",
  "license": "MIT",
  "bugs": {
